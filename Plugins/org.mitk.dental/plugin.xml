<?xml version="1.0" encoding="UTF-8"?>
<plugin>

  <extension id="org.mitk.dental.dentalapplication" point="org.blueberry.osgi.applications">
    <application>
      <run class="DentalApplication"/>
    </application>
  </extension>
  <extension point="org.blueberry.ui.perspectives">
    <perspective
      id="org.mitk.dental.dentalperspective"
      name="Dental Perspective"
      class="DentalPerspective">
    </perspective>
  </extension>
  <extension point="org.blueberry.ui.views">
    <view id="org.mitk.views.mitkview"
          name="MitkView"
          class="MitkView"
          icon="resources/icon.xpm" />
  </extension>

</plugin>
